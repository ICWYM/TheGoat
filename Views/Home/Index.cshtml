        <script>
    // Chức năng cập nhật đồng hồ đếm ngược
            function updateTimer() {
                var currentTime = new Date();
                var endTime = new Date(currentTime);
                endTime.setDate(currentTime.getDate() + 3); //Ngày hiện tại + 3 ngày
                endTime.setHours(12); // Set giờ là  12
                endTime.setMinutes(6); // Set phút là  6
                endTime.setSeconds(2); // Set giây là 2
        
                var timeDifference = endTime - currentTime;
        
                if (timeDifference <= 0) {
                    clearInterval(interval);
                    document.getElementById('day').innerHTML = '0';
                    document.getElementById('hour').innerHTML = '0';
                    document.getElementById('minute').innerHTML = '0';
                    document.getElementById('second').innerHTML = '0';
                    return;
                }
        
                // Tính số ngày, giờ, phút, giây còn lại
                var days = Math.floor(timeDifference / (1000 * 60 * 60 * 24));
                timeDifference %= (1000 * 60 * 60 * 24);
                var hours = Math.floor(timeDifference / (1000 * 60 * 60));
                timeDifference %= (1000 * 60 * 60);
                var minutes = Math.floor(timeDifference / (1000 * 60));
                var seconds = Math.floor(timeDifference / 1000);
        
                // Hiển thị đồng hồ đếm ngược
                document.getElementById('day').innerHTML = formatTime(days);
                document.getElementById('hour').innerHTML = formatTime(hours);
                document.getElementById('minute').innerHTML = formatTime(minutes);
                document.getElementById('second').innerHTML = formatTime(seconds % 60);
            }
        
            //Chức năng định dạng thời gian với số 0 đứng đầu
            function formatTime(time) {
                return time < 10 ? "0" + time : time;
            }
        
           // Lệnh gọi đầu tiên để cập nhật chức năng Hẹn giờ
            updateTimer();


    // Đặt khoảng thời gian gọi cập nhật Chức năng hẹn giờ mỗi giây
            var interval = setInterval(updateTimer, 1000);
        </script>
